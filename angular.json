        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/bulk-registration",
            "index": "apps/bulk-registration/src/index.html",
            "main": "apps/bulk-registration/src/main.ts",
            "polyfills": "apps/bulk-registration/src/polyfills.ts",
            "tsConfig": "apps/bulk-registration/tsconfig.app.json",
            "assets": [
              "apps/bulk-registration/src/favicon.ico",
              "apps/bulk-registration/src/assets"
            ],
            "styles": [
              "apps/bulk-registration/src/styles.scss",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/font-awesome/css/font-awesome.min.css",
              "node_modules/datatables.net-dt/css/jquery.dataTables.css",
              "node_modules/datatables.net-select-dt/css/select.dataTables.css",
              "node_modules/flag-icon-css/css/flag-icon.min.css"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/styles/src/lib/"]
            },
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "node_modules/jquery/dist/jquery.js",
              "node_modules/datatables.net/js/jquery.dataTables.js",
              "node_modules/datatables.net-select/js/dataTables.select.js"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/bulk-registration/src/environments/environment.ts",
                  "with": "apps/bulk-registration/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "15kb"
                }
              ]
            },
            "stage": {
              "fileReplacements": [
                {
                  "replace": "apps/bulk-registration/src/environments/environment.ts",
                  "with": "apps/bulk-registration/src/environments/environment.stage.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "15kb"
                }
              ]
            },
            "dev": {
              "fileReplacements": [
                {
                  "replace": "apps/bulk-registration/src/environments/environment.ts",
                  "with": "apps/bulk-registration/src/environments/environment.dev.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "15kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "bulk-registration:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "bulk-registration:build:production"
            },
            "stage": {
              "browserTarget": "bulk-registration:build:stage"
            },
            "dev": {
              "browserTarget": "bulk-registration:build:dev"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "bulk-registration:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/bulk-registration/src/**/*.ts",
              "apps/bulk-registration/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/bulk-registration"],
          "options": {
            "jestConfig": "apps/bulk-registration/jest.config.js",
            "passWithNoTests": true
